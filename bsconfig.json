{
  "name": "fluid",
  "sources": [{
    "dir": "src",
    "subdirs": [{
      "dir": "native",
      "backend": ["native", "bytecode"]
    }, {
      "dir": "js",
      "backend": ["js"]
    }]
  }
  ],
  "entries": [{
    "backend": "js",
    "main-module": "App"
  }, {
    "backend": "native",
    "main-module": "AppNative"
  }],
  "build-script": "build-script.re",
  "bsc-flags": ["-g"],
  "warnings": {
    "number": "-27-45-40-26-32-44"
  },
  "bs-dependencies": ["flex"],
  "package-specs": {
    "module": "commonjs",
    "in-source": true
  },
  "suffix": ".bs.js",
  "c-linker-flags": ["-framework", "Cocoa"],
  "static-libraries": ["lib/fluid.o"],
  "refmt": 3,
  "ppx-flags": ["./fluid-ppx/lib/bs/native/fluid_ppx.native"]
}
